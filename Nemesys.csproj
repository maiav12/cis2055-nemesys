    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-Nemesys-315f4851-4be1-400b-b2dc-8bfcc60c1e9a</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\lib\bootstrap\css\styles.cs" />
  </ItemGroup>

  <ItemGroup>
